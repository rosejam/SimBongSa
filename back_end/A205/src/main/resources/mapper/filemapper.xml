<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.a205.model.filemapper">

	<insert id="insertFile" parameterType="java.util.Map">
		insert into
		`file`(p_id, f_name, f_key, f_size)
		values(#{p_id}, #{originalFileName}, #{storedFileName}, #{file_size})
	</insert>
	
<!-- 	<select id="selectFileList" parameterType="int">
		select f_id, f_name, f_key, round(f_size/1024, 1)
		from `file`
		where p_id = #{p_id}
	</select> -->
	<select id="selectFileList" resultType="java.lang.String" parameterType="int">
		select f_key
		from `file`
		where p_id = #{p_id}
	</select>

</mapper>